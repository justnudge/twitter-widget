<iw:iwidget name="com.justnudge.lcc.TwitterWidget" 
            xmlns:iw="http://www.ibm.com/xmlns/prod/iWidget" 
            supportedModes="view"
            mode="view"
            allowInstanceContent="true"
            iScope="com.justnudge.lcc.TwitterWidget">
    <iw:itemSet id="attributes">
        <iw:item id="messageURI" value="https://raw.github.com/justnudge/messages/master" />
        <iw:item id="imageURI" value="https://raw.github.com/justnudge/twitter-widget/master" />
    </iw:itemSet>
    <iw:resource uri="TwitterWidget.js" />
    <iw:resource uri="jquery.min.js" />
    <iw:resource uri="twitter.js" />
    <iw:resource uri="twitter.css" />
    <iw:content mode="view">
        <![CDATA[
    <div id="jnTwitterDIV"></div>
        ]]>
    </iw:content>
</iw:iwidget>